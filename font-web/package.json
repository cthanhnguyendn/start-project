  },
  "dependencies": {
    "axios": "0.9.1",
    "deep-equal": "1.0.1",
    "jquery": "^2.2.3",
    "react": "15.0.0",
    "react-addons-shallow-compare": "15.0.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "15.0.0",
    "react-dropzone": "^3.4.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "4.0.2",
    "react-virtualized": "^7.1.3",
    "redux": "3.3.1",
    "redux-form": "^5.2.3"
